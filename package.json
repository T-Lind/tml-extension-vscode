{
  "name": "turing-machine-language",
  "displayName": "Turing Machine Language",
  "description": "Support for code highlighting of the Turing Machine Language",
  "version": "1.0.0",
  "icon": "./icons/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/T-Lind/tml-extension-vscode.git"
},
"homepage": "https://txkl.gitbook.io/tml/",
  "publisher": "T-Lind",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Themes"
  ],
  "galleryBanner": {
    "color": "#912d2f",
    "theme": "dark",
    "image": "./images/tml-banner.png"
  },
  "contributes": {
    "languages": [
      {
          "id": "turing-machine-language",
          "extensions": [".tml"],
          "aliases": ["Turing Machine Language"],
          "configuration": "./language-configuration.json"
      }
  ],
  "grammars": [
      {
          "language": "turing-machine-language",
          "scopeName": "source.turing-machine-language",
          "path": "./turing-machine-language.tmLanguage.json"
      }
  ],
    "themes": [
      {
        "label": "Turing Machine Language Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/Turing Machine Language Theme-color-theme.json"
      }
    ]
  }
}
